    "babel-preset-react": "^6.1.18",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.5",
    "jsdom": "^7.1.0",
    "minimist": "^1.2.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "open": "0.0.5",
