{
  "name": "react-sound",
  "version": "1.0.0",
  "description": "SoundCloud App",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/helpers/test_helper.js \"test/**/*.@(js|jsx)\"",
    "start": "babel-node ./server.js --env=dev",
    "start:dist": "babel-node ./server.js --env=dist",
