{
  "name": "react-sound",
  "version": "1.0.0",
  "description": "SoundCloud App",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:dist",
    "build:dev": "NODE_ENV=development webpack --config ./webpack/config.babel.js --server",
    "build:dist": "NODE_ENV=production webpack --config ./webpack/config.babel.js",
    "clean": "rimraf dist/*",
    "client": "NODE_ENV=development babel-node ./server/index.js --client",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dev": "NODE_ENV=development babel-node ./server/index.js",
    "dist": "NODE_ENV=production npm run build && babel-node ./server/index.js",
    "lint": "eslint ./src/scripts --ext=js --ext=jsx",
    "start": "NODE_ENV=development nodemon",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/helpers/test_helper.js \"test/**/*.@(js|jsx)\""
  },
  "author": "Socrates",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "^8.3.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.8.2",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "cookie-parser": "^1.4.1",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.0",
    "lodash": "^4.8.2",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "normalizr": "^2.0.1",
    "pretty-error": "^2.0.0",
    "query-string": "^4.1.0",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-helmet": "^3.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "susy": "^2.2.12"
  }
}
